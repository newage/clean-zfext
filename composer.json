{
    "require": {
        "newage/zf-scaffold": "dev-master",
        "zend/zf1": "1.12.0",
        "zfdebug": "1.5"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@bitbucket.org:newage/zf-scaffold.git"
        },
        {
            "type": "package",
            "package": {
                "name": "zend/zf1",
                "version": "1.12.0",
                "dist": {
                    "url": "http://framework.zend.com/releases/ZendFramework-1.12.0/ZendFramework-1.12.0.zip",
                    "type": "zip"
                },
                "autoload": {
                    "psr-0": {
                        "Zend": "library",
                        "ZendX": "extras/library"
                    }
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "zfdebug",
                "version": "1.5",
                "dist": {
                    "url": "https://bitbucket.org/newage/zf-debug/get/master.zip",
                    "type": "zip"
                },
                "autoload": {
                    "psr-0": {
                        "ZFDebug": "library"
                    }
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "newage/zf-migration",
                "version": "1.5",
                "dist": {
                    "url": "https://bitbucket.org/newage/zf-debug/get/master.zip",
                    "type": "zip"
                },
                "autoload": {
                    "psr-0": {
                        "ZFDebug": "library"
                    }
                }
            }
        }
    ]
}

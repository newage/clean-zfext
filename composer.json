{
    "require": {
        "zend/zf1": "1.11.12",
        "newage/zf-scaffold": "1.0.1"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "zend/zf1",
                "version": "1.11.12",
                "dist": {
                    "url": "http://framework.zend.com/releases/ZendFramework-1.11.12/ZendFramework-1.11.12.zip",
                    "type": "zip"
                },
                "autoload": {
                    "psr-0": {
                        "Zend": "library",
                        "ZendX": "extras/library"
                    }
                }
            }
        },
        {
            "type": "package",
	    "package": {
                "name": "newage/zf-scaffold",
                "version": "1.0.1",
                "dist": {
                    "url": "https://bitbucket.org/newage/zf-scaffold/get/1.0.1.zip",
                    "type": "zip"
                },
                "autoload": {
                    "psr-0": {
                        "ZFScaffold": "library"
                    }
                }
            }
        },
        {
            "type": "package",
	    "package": {
                "name": "zfdebug",
                "version": "1.5",
                "dist": {
                    "url": "https://bitbucket.org/newage/zf-debug/get/master.zip",
                    "type": "zip"
                },
                "autoload": {
                    "psr-0": {
                        "ZFDebug": "library"
                    }
                }
            }
        }
    ]
}

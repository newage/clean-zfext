{
    "require": {
        "newage/zf-tools": "dev-master",
        "newage/zf-debug": "dev-master",
        "zend/zf1": "1.12.2",
        "components/jquery": "1.8.*",
        "components/twitter-bootstrap": "2.2.*",
        "components/select2": "3.2",
        "components/bootstrap-notify": "1.*",
        "components/bootstrap-wysihtml5": "1.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@bitbucket.org:newage/zf-tools.git"
        },
        {
            "type": "package",
            "package": {
                "name": "zend/zf1",
                "version": "1.12.2",
                "dist": {
                    "url": "http://framework.zend.com/releases/ZendFramework-1.12.2/ZendFramework-1.12.2.zip",
                    "type": "zip"
                },
                "autoload": {
                    "psr-0": {
                        "Zend": "library",
                        "ZendX": "extras/library"
                    }
                }
            }
        },
        {
            "type": "vcs",
            "url":  "git@bitbucket.org:newage/zf-debug.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:components/jquery.git"
        },
        {
            "type": "package",
            "package": {
                "name": "components/twitter-bootstrap",
                "version": "2.2.2",
                "dist": {
                    "url": "http://twitter.github.com/bootstrap/assets/bootstrap.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "components/select2",
                "version": "3.2",
                "dist": {
                    "url": "https://github.com/ivaynberg/select2/archive/release-3.2.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "components/bootstrap-notify",
                "version": "1.0.0",
                "dist": {
                    "url": "https://github.com/Nijikokun/bootstrap-notify/archive/1.0.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "components/bootstrap-wysihtml5",
                "version": "1.0.0",
                "dist": {
                    "url": "https://github.com/jhollingworth/bootstrap-wysihtml5/archive/master.zip",
                    "type": "zip"
                }
            }
        }
    ]
}

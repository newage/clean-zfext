{
    "require": {
        "newage/zf-tools": "1.*",
        "newage/zf-console": "1.*",
        "zend/zf1": "1.12.*",
        "components/jquery": "1.9.*",
        "components/bootstrap": "2.3.*",
        "components/bootstrap-default": "2.3.*",
        "components/font-awesome": "3.2.*",
        "ivaynberg/select2": "3.*",
        "Nijikokun/bootstrap-notify": "1.*",
        "newage/jquery-i18n": "1.*",
        "eternicode/bootstrap-datepicker": "1.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:newage/zf-tools.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:newage/zf-console.git"
        },
        {
            "type": "package",
            "package": {
                "name": "ivaynberg/select2",
                "type": "component",
                "version": "3.4.1",
                "dist": {
                    "url": "https://github.com/ivaynberg/select2/archive/3.4.1.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/ivaynberg/select2",
                    "type": "git",
                    "reference": "3.4.1"
                },
                "extra": {
                    "component": {
                        "scripts": [
                            "select2.js"
                        ],
                        "styles": [
                            "select2.css"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "Nijikokun/bootstrap-notify",
                "type": "component",
                "version": "1.0.0",
                "dist": {
                    "url": "https://github.com/Nijikokun/bootstrap-notify/archive/1.0.0.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/Nijikokun/bootstrap-notify",
                    "type": "git",
                    "reference": "1.0.0"
                },
                "extra": {
                    "component": {
                        "scripts": [
                            "js/bootstrap-notify.js"
                        ],
                        "styles": [
                            "css/bootstrap-notify.css"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "newage/jquery-i18n",
                "type": "component",
                "version": "1.0.0",
                "dist": {
                    "url": "https://github.com/newage/jquery-i18n/archive/v1.0.0.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/newage/jquery-i18n",
                    "type": "git",
                    "reference": "1.0.0"
                },
                "extra": {
                    "component": {
                        "scripts": [
                            "jquery.i18n.js"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "eternicode/bootstrap-datepicker",
                "type": "component",
                "version": "1.0.1",
                "dist": {
                    "url": "https://github.com/eternicode/bootstrap-datepicker/zipball/v1.0.1",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/eternicode/bootstrap-datepicker",
                    "type": "git",
                    "reference": "1.0.0"
                },
                "extra": {
                    "component": {
                        "scripts": [
                            "jquery.i18n.js"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "zend/zf1",
                "version": "1.12.3",
                "dist": {
                    "url": "http://framework.zend.com/releases/ZendFramework-1.12.3/ZendFramework-1.12.3.zip",
                    "type": "zip"
                },
                "autoload": {
                    "psr-0": {
                        "Zend": "library"
                    }
                }
            }
        }
    ]
}

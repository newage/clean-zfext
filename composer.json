{
    "require": {
        "newage/zf-tools": "1.*",
        "newage/zf-debug": "1.*",
        "zend/zf1": "1.12.*",
        "view/bootstrap": "2.3.*",
        "view/select2": "3.*",
        "view/bootstrap-notify": "1.*",
        "view/font-awesome": "3.*",
        "view/jquery-i18n": "1.*",
        "view/bootstrap-dropdown": "1.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@bitbucket.org:newage/zf-tools.git"
        },
        {
            "type": "package",
            "package": {
                "name": "view/bootstrap-dropdown",
                "version": "1.0.0",
                "dist": {
                    "url": "http://lightglitch.github.io/bootstrap-xtra/js/bootstrap-dropdown.js",
                    "type": "file"
                },
                "require": {
                    "view/bootstrap": ">=2.3.1"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "view/jquery-i18n",
                "version": "1.0.1",
                "dist": {
                    "url": "https://github.com/newage/jquery-i18n/archive/develop.zip",
                    "type": "zip"
                },
                "require": {
                    "view/jquery": ">=1.4.1"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "zend/zf1",
                "version": "1.12.3",
                "dist": {
                    "url": "http://framework.zend.com/releases/ZendFramework-1.12.3/ZendFramework-1.12.3.zip",
                    "type": "zip"
                },
                "autoload": {
                    "psr-0": {
                        "Zend": "library",
                        "ZendX": "extras/library"
                    }
                }
            }
        },
        {
            "type": "vcs",
            "url":  "git@bitbucket.org:newage/zf-debug.git"
        },
        {
            "type": "package",
            "package": {
                "name": "view/jquery",
                "version": "1.9.1",
                "dist": {
                    "url": "http://code.jquery.com/jquery-1.9.1.min.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "view/bootstrap",
                "version": "2.3.1",
                "dist": {
                    "url": "http://twitter.github.io/bootstrap/assets/bootstrap.zip",
                    "type": "zip"
                },
                "require": {
                    "view/jquery": "1.9.*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "view/select2",
                "version": "3.3.2",
                "dist": {
                    "url": "https://github.com/ivaynberg/select2/archive/3.3.2.zip",
                    "type": "zip"
                },
                "require": {
                    "view/bootstrap": "2.*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "view/bootstrap-notify",
                "version": "1.0.0",
                "dist": {
                    "url": "https://github.com/Nijikokun/bootstrap-notify/archive/1.0.0.zip",
                    "type": "zip"
                },
                "require": {
                    "view/bootstrap": "2.*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "view/font-awesome",
                "version": "3.2.1",
                "dist": {
                    "url": "http://fortawesome.github.io/Font-Awesome/assets/font-awesome.zip",
                    "type": "zip"
                }
            }
        }
    ]
}

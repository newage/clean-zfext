{
    "require": {
        "newage/zf-tools": "1.*",
        "newage/zf-debug": "1.*",
        "zend/zf1": "1.12.*",
        "components/jquery": "1.9.*",
        "components/twitter-bootstrap": "2.3.*",
        "components/select2": "3.*",
        "components/bootstrap-notify": "1.*",
        "components/font-awesome": "3.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@bitbucket.org:newage/zf-tools.git"
        },
        {
            "type": "package",
            "package": {
                "name": "zend/zf1",
                "version": "1.12.2",
                "dist": {
                    "url": "http://framework.zend.com/releases/ZendFramework-1.12.2/ZendFramework-1.12.2.zip",
                    "type": "zip"
                },
                "autoload": {
                    "psr-0": {
                        "Zend": "library",
                        "ZendX": "extras/library"
                    }
                }
            }
        },
        {
            "type": "vcs",
            "url":  "git@bitbucket.org:newage/zf-debug.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:components/jquery.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:components/bootstrap.git"
        },
        {
            "type": "package",
            "package": {
                "name": "components/select2",
                "version": "3.3.1",
                "dist": {
                    "url": "https://github.com/ivaynberg/select2/archive/3.3.1.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "components/bootstrap-notify",
                "version": "1.0.0",
                "dist": {
                    "url": "https://github.com/Nijikokun/bootstrap-notify/archive/1.0.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "components/font-awesome",
                "version": "3.0.2",
                "dist": {
                    "url": "https://github.com/FortAwesome/Font-Awesome/zipball/master",
                    "type": "zip"
                }
            }
        }
    ]
}
